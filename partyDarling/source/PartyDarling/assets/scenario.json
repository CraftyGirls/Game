      "name": "EvelynQuestComplete",
      "dialogue": [
        {
          "text": [
            "Oh did you count them?"
          ],
          "triggers": [],
          "conditions": [],
          "speaker": 3,
          "name": "Dialogue 0"
        }
      ],
      "options": [
        {
          "convoId": 8,
